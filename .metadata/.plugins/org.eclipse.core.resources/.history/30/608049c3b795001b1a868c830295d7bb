package ex02.inheritance;

public class Resct extends Point {
	private int x2;
	private int y2;
	// 생성자 함수 = 2개이상 
	private Resct(int z) {
		this.y2 = 1000;
	}
	
	Resct() {
		this.x2 =30;
	}


	//getter //setter method 
	
	public int getZ() {
		return z;
	}

	public void setZ(int z) {
		this.z = z;
	}
	
	public  void disp() {
		System.out.println(x+","+z);
	}
	
	//output method 
	
}
